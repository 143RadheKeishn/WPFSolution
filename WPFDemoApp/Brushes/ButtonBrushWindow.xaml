<Window x:Class="WPFDemoApp.Brushes.ButtonBrushWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDemoApp.Brushes"
        mc:Ignorable="d"
        Title="ButtonBrushWindow" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"></RowDefinition>
            <RowDefinition Height="3*"></RowDefinition>
            <RowDefinition Height="5*"></RowDefinition>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Content="This is my Button" Background="{StaticResource  Lbrush1}"></Button>
        <Button Grid.Row="1" Content="This is my Button" Background="{StaticResource  Lbrush1}"></Button>
        <Button Grid.Row="2" Content="This is my Button" Background="{StaticResource  vikas}"></Button>
    </Grid>
</Window>
